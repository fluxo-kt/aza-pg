{
  "$schema": "https://json.schemastore.org/oxlintrc",
  "env": {
    "node": true,
    "es2024": true
  },
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "perf": "warn"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-useless-escape": "error",
    "no-empty": "error",
    "no-unreachable": "error",
    "use-isnan": "error",
    "no-self-compare": "error",
    "no-console": "warn",
    "prefer-const": "warn",
    "eqeqeq": "warn",
    "no-eval": "warn",
    "no-with": "warn"
  }
}
